package icons

templ BaseIcon(optionalClasses ...string) {
	{{
allClasses := "size-6"
for _, c := range optionalClasses {
	allClasses += " " + c
}
	}}
	<svg
		xmlns="http://www.w3.org/2000/svg"
		viewBox="0 0 24 24"
		fill="none"
		stroke="currentColor"
		stroke-width="2"
		stroke-linecap="round"
		stroke-linejoin="round"
		class={ allClasses }
	>
		{ children... }
	</svg>
}
